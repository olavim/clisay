print(if true { "hello" } else { "world" }); // expect: hello
print(if false { "hello" } else { "world" }); // expect: world
print(if true { "hello" }); // expect: hello
print(if false { "hello" }); // expect: null
print(if true { }); // expect: null
print(if false { } else { }); // expect: null

print(if true "hello" else "world"); // expect: hello
print(if false "hello" else "world"); // expect: world
print(if true "hello"); // expect: hello
print(if false "hello"); // expect: null
