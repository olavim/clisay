fn isOne(n) {
    if (n == 1) {
        return true;
    }
    return false;
}

fn isOneOrTwo(n) {
    if (n == 1) {
        return true;
    } else if (n == 2) {
        return true;
    } else {
        return false;
    }
}

print(isOne(1)); // expect: true
print(isOne(2)); // expect: false
print(isOneOrTwo(1)); // expect: true
print(isOneOrTwo(2)); // expect: true
print(isOneOrTwo(3)); // expect: false

say i = 0;
while (i < 3) {
    // expect: 1
    // expect: 2
    // expect: 3
    print(i += 1);
}
